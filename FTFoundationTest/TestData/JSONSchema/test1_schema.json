                    }
                }
            }
        },

        "no_check" : { "type" : "any", "required":true},

        "string_list" : {
            "type" : "array",
            "required":true,
            "items" : {
                "type":"string", "required":true
            }
        },

        "any_list" : {
            "type" : "array",
            "items" : { "type":"any" }
        },

        "obj_list" : {
            "required":true,
            "type" : "array",
            "items" : {
                "type" : "object",
                    "properties" : {
                        "name" : { "type":"string", "required":true},
                        "age"    : { "type":"integer", "required":true}
                    }
            }
        },

        "multitype_list" : {
            "required":true,
            "type" : "array",
